{{
  config(
    materialized='view'
  )
}}

WITH src_products AS (
    SELECT * 
    FROM {{ source('sql_server_dbo', 'products') }}
    ),

products_renamed AS (
    SELECT
          product_id as id_producto
        , name as Producto_Nombre
        , inventory as Producto_Cantidad
        , price as Producto_Precio
        
    FROM src_products
    )

SELECT * FROM products_renamed